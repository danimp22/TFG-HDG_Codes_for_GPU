function plotVelocityVectorsElements(X,T,u,referenceElement_star)

%Basis functions of standard element at star element nodes for
%interpolation of X
N=evaluateNodalBasisTriwithoutDerivatives(XI,XInodes,referenceElement_star.degree);

Xs=[];
for i=1:size(T,1), Xs=[Xs;X(T(i,:),:)]; end
quiver(Xs(:,1),Xs(:,2),u(:,1),u(:,2));